{
  "name": "asterism-plugin-zwave",
  "version": "1.1.3",
  "description": "An asterism plugin to add Z-wave support",
  "author": "Xavier Gouley <gxapplications@gmail.com>",
  "contributors": [],
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "dist": "rimraf dist && babel lib --comments false --compact true --out-dir dist",
    "wrap": "npm run dist && cd ../asterism/ && npm install --no-save file:../asterism-plugin-zwave",
    "start": "cd ../asterism/ && cross-env ASTERISM_PLUGINS=asterism-plugin-zwave npm start",
    "start:debug": "cd ../asterism/ && cross-env ASTERISM_PLUGINS=asterism-plugin-zwave npm run start:debug",
    "prepublishOnly": "npm run dist",
    "lint": "standard --verbose | snazzy",
    "validate": "npm ls"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxapplications/asterism-plugin-zwave"
  },
  "bugs": {
    "url": "https://github.com/gxapplications/asterism-plugin-zwave/issues"
  },
  "homepage": "https://github.com/gxapplications/asterism-plugin-zwave",
  "keywords": [
    "zwave",
    "z-wave",
    "asterism",
    "domotics"
  ],
  "engines": {
    "node": "8.13.0 - 8.17.0",
    "npm": ">= 6.4.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": "71"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "css-modules-transform",
            {
              "preprocessCss": "../asterism-plugin-zwave/babel-sass-preprocess.js",
              "generateScopedName": "[local]",
              "extractCss": "../asterism-plugin-zwave/dist/styles.css",
              "extensions": [
                ".css",
                ".sass",
                ".scss"
              ]
            }
          ]
        ]
      }
    }
  },
  "standard": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/doc/"
    ]
  },
  "dependencies": {
    "chart.js": "2.7.x",
    "debounce": "1.x",
    "joi": "13.x",
    "joi-browser": "13.x",
    "openzwave-shared": "1.5.10",
    "os": "0.1.x",
    "path": "0.12.x"
  },
  "devDependencies": {
    "asterism-plugin-library": ">= 2.3.4",
    "babel-cli": "6.26.x",
    "babel-core": "6.26.x",
    "babel-eslint": "6.1.x",
    "babel-plugin-css-modules-transform": "1.5.0",
    "babel-plugin-transform-object-rest-spread": "6.23.x",
    "babel-preset-env": "1.6.x",
    "babel-preset-react": "6.24.x",
    "cross-env": "^5.x",
    "express": "4.16.x",
    "node-sass": "4.13.x",
    "prop-types": "15.6.x",
    "react": "16.2.x",
    "react-materialize": "3.3.3",
    "rimraf": "2.x",
    "snazzy": "7.x",
    "standard": "10.x"
  },
  "peerDependencies": {
    "asterism": ">= 1.3.0",
    "asterism-plugin-library": ">= 2.3.4",
    "express": "4.16.x",
    "prop-types": "15.6.x",
    "react": "16.2.x",
    "react-materialize": "3.3.3"
  },
  "directories": {
    "lib": "./lib",
    "dist": "./dist"
  },
  "config": {}
}
